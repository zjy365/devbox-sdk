{
  "name": "devbox-sdk",
  "version": "1.1.2",
  "description": "Enterprise TypeScript SDK for Sealos Devbox management",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "sealos",
    "devbox",
    "sdk",
    "typescript",
    "cloud-development",
    "container",
    "http-api"
  ],
  "author": {
    "name": "zjy365",
    "email": "3161362058@qq.com",
    "url": "https://github.com/zjy365"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/zjy365/devbox-sdk#readme",
  "bugs": {
    "url": "https://github.com/zjy365/devbox-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zjy365/devbox-sdk.git",
    "directory": "packages/sdk"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "devbox-shared": "workspace:*",
    "tsup": "^8.0.0"
  }
}